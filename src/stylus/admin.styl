body
  background #666
  margin 1em


table.show
  table-layout fixed
  width 90%
  border 1px solid black
  border-collapse collapse
  margin 0 auto

  td, th
    border 1px solid black

  tr:nth-child(odd)
    background #777



#ad_buttons
  display flex
  background #333
  justify-content space-between
  width 100%
  margin-bottom 2em

#ad_init
  background red
  // width 300px

#ad_show
  background blue

.mapRef
  width 10%
  overflow hidden
  text-overflow ellipsis
